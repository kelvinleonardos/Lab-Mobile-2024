<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#DED0B6">

    <RelativeLayout
        android:id="@+id/relative_rl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp">

        <ImageView
            android:id="@+id/imageViewLeft"
            android:layout_width="43dp"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:src="@drawable/logounhas" />

        <TextView
            android:id="@+id/textViewTitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@id/imageViewLeft"
            android:layout_toStartOf="@id/imageViewRight"
            android:fontFamily="times"
            android:gravity="center"
            android:paddingTop="5dp"
            android:text="Data Pribadi"
            android:textColor="#3C3633"
            android:textSize="23sp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imageViewRight"
            android:layout_width="44dp"
            android:layout_height="40dp"
            android:layout_alignParentEnd="true"
            android:src="@drawable/baseline_account_circle_24" />
    </RelativeLayout>
</RelativeLayout>
